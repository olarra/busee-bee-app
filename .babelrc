{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source",
        [
          "module-resolver",
          {
            "root": ["./src"],
            "extensions": [".tsx", ".ts", ".js", ".ios.js", ".android.js"],
            "alias": {
              "@config": "./src/config",
              "@shared": "./src/shared"
            }
          }
        ]
      ]
    }
  }
}
